{
  "name": "react-slidebar",
  "version": "1.0.0",
  "description": "React UI component for \"sidebar\" with \"slide\" transition between nested menu items",
  "main": "dist/react-slidebar.umd.js",
  "unpkg": "dist/react-slider.umd.min.js",
  "scripts": {
    "dev": "parcel demo/index.html -d .devserver --no-cache",
    "build": "rollup --config",
    "lint": "eslint \"{src,test}/**/*.js\"",
    "format": "prettier --write \"{src,test,demo}/**/*.{js,json,css,md}\"",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monteith/react-slidebar.git"
  },
  "keywords": [
    "react",
    "ui",
    "menu"
  ],
  "author": "Graham Monteith <monteith.g@gmail.com> (http://g12h.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/monteith/react-slidebar/issues"
  },
  "homepage": "https://github.com/monteith/react-slidebar#readme",
  "files": [
    "dist/"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>test/setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,demo}/**/*.{js,ts,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "{src,test}/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-spring": "8.0.27"
  },
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-spring": "8.0.27"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@emotion/styled": "10.0.27",
    "@rollup/plugin-babel": "5.0.2",
    "@rollup/plugin-node-resolve": "8.0.0",
    "@rollup/plugin-replace": "2.3.2",
    "@testing-library/react": "10.0.4",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "core-js": "3.6.5",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.7",
    "parcel": "1.12.4",
    "prettier": "2.0.5",
    "rollup": "2.11.2",
    "rollup-plugin-terser": "6.1.0"
  }
}